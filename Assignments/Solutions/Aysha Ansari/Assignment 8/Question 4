class Solution
{
  public:
    string modify (string s)
    {
        int n = s.length ();
        int i = 0, itr = 0;
    
        while (i < n)
        {
            if (s[i] != ' '){ s[itr] = s[i];
                itr++;
            }
            i++;
        }
        return s.substr (0, itr);
    }
}
